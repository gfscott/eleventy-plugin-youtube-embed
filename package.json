  },
  "bugs": "https://github.com/gfscott/eleventy-plugin-youtube-embed/issues",
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "ava": "^5.1.1",
    "nyc": "^15.1.0"
  },
